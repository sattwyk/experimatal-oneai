{
  "name": "experimental-oneai",
  "version": "1.0.0",
  "description": "THIS IS AN EXPERIMENTAL APP",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app": "npm run-script start --workspace=app",
    "api": "npm run-script start --workspace=api",
    "start": "concurrently --kill-others-on-fail \"npx run app\"  \"npx run api\""
  },

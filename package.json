  "bugs": {
    "url": "https://github.com/sattwyk/experimental-oneai/issues"
  },
  "homepage": "https://github.com/sattwyk/experimental-oneai#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
